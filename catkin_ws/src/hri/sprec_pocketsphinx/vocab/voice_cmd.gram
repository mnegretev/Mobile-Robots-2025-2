#JSGF V1.0;

/**
 * JSGF Grammar similar to kws example
 */

grammar voice_cmd;

public <move> = MOVE FORWARD;

public <move2> = STOP | MOVE <direction> [<speed>];

<direction> = FORWARD | BACK | LEFT | RIGHT;

<speed> = FULL SPEED | HALF SPEED;


grammar final_project_gram;

public <simple_command> = ROBOT TAKE THE <object> TO THE <location>;

<object> = PRINGLES | DRINK;
<location> = TABLE | KITCHEN;